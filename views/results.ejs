    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous">

     
    <!-- Bing Maps Script -->
    <script type='text/javascript' src='https://www.bing.com/api/maps/mapcontrol?key=AlFKiB22ZpOAxUyuOOwz0wEON7N_Elwszb4PA29tc9Drag0C1sf5W8zUfmbhJ9Lg&callback=loadMapScenario' async defer></script>
    
    

    <title>Home Search Page</title>
  </head>
  <body>


<br><br><br>
  <div class="container">
    <div class="col-md-12">
      <div class="card "> <!-- Creates grey border-->
        <div class="row"> <!--Row 1-->
          
          <div class="col-md-6"> <!--Column 1-->
            <!--Row 1, Column 1--><br>
            <H3 class="d-inline-block mb-2 text-primary">Lindsey Turner with Wilson Reality</H3>
            <p class="lead">
                <p>
                 <%= streetNumberText %> <%= streetName %> <br>
                 <%= city %>, <%= state %> <%= postalCode %>
               </p>
              </p>
             <div class="text-muted">          
                Listed Price: $ <%= listPrice %>
             </div>
          </div> <!-- end of Column 1-->
          
          <div class="col-md-5"><!--Column 2-->
              <!--Row 1, Column 2-->
              <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                  
                
                  <div class="carousel-inner">
                      <div class="carousel-item active">
                          <img class="d-block w-100" src="<%- photos[0]%>" alt="First slide" class="img-fluid">
                        </div>

                      <% for(var i = 1; i < photos.length; ++i) 
                      {%>
                        <div class="carousel-item">
                            <img class="d-block w-100" src="<%- photos[i]%>" alt="First slide" class="img-fluid">
                          </div>
                        
                        <%} %>
                  </div>
                  <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                  </a>
                  <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                  </a>
                </div>
          <!--  <% {%><img class="img-fluid" src="<%- photos[0]%>"></img><% } %>  -->
          </div><!-- end of Column 2-->
          
          <div class="col-sm-1"><!--Column 3-->
            <!--Row 1, Column 3-->
          </div><!-- end of Column 3-->
        </div><!--End of Row 1-->
        
        <div class="row"> <!--Row 2-->
          <div class="col-md-6"> <!--Column 1-->
            <!--Row 2, Column 1--><br>
            <p class="flex-column"><%= remarks %></p>
          </div>
          
          <div class="col-md-5"><!--Column 2-->
            <!--Row 2 Column 2--> <br>
            <div class="flex-column" id='myMap' style='width: 23vw; height:30vh;'></div>
            
            <script type='text/javascript'>
                function loadMapScenario() {
                    var navigationBarMode = Microsoft.Maps.NavigationBarMode;
                    var map = new Microsoft.Maps.Map(document.getElementById('myMap'), {
                        /* No need to set credentials if already passed in URL */
                        navigationBarMode: navigationBarMode.compact,
                        supportedMapTypes: [Microsoft.Maps.MapTypeId.road, Microsoft.Maps.MapTypeId.aerial, Microsoft.Maps.MapTypeId.canvasLight],
                       center: new Microsoft.Maps.Location (29.798412, -95.756254), zoom: 15,
                       
                      });
                }
            </script>
          </div> <!--end of Column 2-->
          
          <div class="col-sm-1"> <!--Column 3-->
            <!--Row 2 Column 3-->
              <p>                   </p>
          </div> <!--end of Column 3-->

        </div> <!--End of Row 2-->


        <div class="row"> <!--Row 3-->
          <div class="col-md-12"> <!--Column 1-->
            <!--Row 3 Column 1-->
           <p>Brokered by: Wilson Reality</p>
            <a id="popoverOption" class="" href="#" data-content="<%- disclaimer %>" rel="popover" data-placement="bottom" data-original-title="Disclaimer and Legal Information">Disclaimer</a>  
            <button type="button" class="btn-sm btn-outline-primary " aria-pressed="true">Favorite</button>
          </div> <!--end of Column 1-->
        </div> <!--Row 3-->
      </div>

    </div>
  </div>

    
    <script>
        $(document).ready(function(){
          
          $('.carousel').carousel()
        </script>
    
    

    <script>
    $(document).ready(function(){
      
      $('#popoverOption').popover({ trigger: "hover" }); 
    });
    </script>



    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js" integrity="sha384-smHYKdLADwkXOn1EmN1qk/HfnUcbVRZyYmZ4qpPea6sjB/pTJ0euyQp0Mk8ck+5T" crossorigin="anonymous"></script>


  </body>
</html>

